        fetch('/user/friends')
            .then(response => response.json())
            .then(friends => {
                const chatUsersContainer = document.getElementById('chat-users');
                friends.forEach(friend => {
                    const li = document.createElement('li');
                    li.className = 'friend-item position-relative'; // Thêm lớp Bootstrap
                    li.setAttribute('data-friend-id', friend.friend_id._id);
                    li.innerHTML = `
                        <div class="friend-thumbnail">
                            <img src="/${friend.friend_id.profile_picture || '/images/default_profile.png'}" 
                                alt="${friend.friend_id.username}" 
