<style>
    #friendProfilePic {
        width: 35px; /* Chiều rộng */
        height: 35px; /* Chiều cao */
        object-fit: cover; /* Đảm bảo ảnh không bị biến dạng */
        border-radius: 50%; /* Tạo hình tròn */
    }
    .btn-like {
        border: none;
        background: transparent;
        color: #65676b;
        font-size: 14px;
        cursor: pointer;
        transition: color 0.2s ease, transform 0.2s ease;
    }

    .btn-like.liked {
        color: #1877f2; /* Màu xanh Facebook */
    }

    .like-count i {
        margin-right: 5px;
        font-size: 16px;
        transition: color 0.2s ease;
    }

    .like-count.liked i {
        color: #1877f2; /* Màu xanh Facebook khi liked */
    }

    .btn-like:hover, .like-count:hover {
        transform: scale(1.1);
    }

</style>
